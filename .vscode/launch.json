	"version": "0.2.0",
	"configurations": [
		{
			"name": "(lldb) Launch CLI",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/OSX/Lib/CLI.o",
			"internalConsoleOptions": "openOnSessionStart",
			"args": [],
			"stopAtEntry": true,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": true,
			"MIMode": "lldb",
			"miDebuggerPath": "/Applications/Xcode.app/Contents/Developer/usr/bin/lldb-mi"
		}
	]
}